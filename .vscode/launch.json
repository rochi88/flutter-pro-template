    "version": "0.2.0",
    "configurations": [
      {
        "name": "Launch development",
        "request": "launch",
        "type": "dart",
        "program": "lib/main_dev.dart",
        "args": [
          "--flavor",
          "development",
          "--target",
          "lib/main_dev.dart"
        ]
      },
      {
        "name": "Launch staging",
        "request": "launch",
        "type": "dart",
        "program": "lib/main_stg.dart",
        "args": ["--flavor", "staging", "--target", "lib/main_stg.dart"]
      },
      {
        "name": "Launch production",
        "request": "launch",
        "type": "dart",
        "program": "lib/main.dart",
        "args": ["--flavor", "production", "--target", "lib/main.dart"]
      }
    ]
  }